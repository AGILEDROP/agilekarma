{
  "name": "working-plusplus",
  "version": "0.0.4",
  "description": "Like plusplus.chat, but one that actually works, because you can host it yourself ðŸ˜‰",
  "main": "index.ts",
  "repository": "git@github.com:tdmalone/working-plusplus.git",
  "author": "Tim Malone <tdmalone@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "serve": "node ./dist/index.js",
    "dev": "nodemon",
    "lint": "./node_modules/.bin/eslint . --ext .ts",
    "test": "jest --forceExit --runInBand",
    "db-migrate-up": "ts-node-esm ./node_modules/.bin/knex migrate:up",
    "db-migrate-down": "ts-node-esm ./node_modules/.bin/knex migrate:down",
    "db-migrate-latest": "ts-node-esm ./node_modules/.bin/knex migrate:latest",
    "unit-tests": "SKIP_INTEGRATION_TESTS=true SKIP_E2E_TESTS=true jest --forceExit",
    "integration-tests": "SKIP_E2E_TESTS=true jest --forceExit integration-tests",
    "e2e-tests": "SKIP_INTEGRATION_TESTS=true jest --forceExit e2e-tests",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "engines": {
    "node": "^16"
  },
  "type": "module",
  "keywords": [
    "slack",
    "bot",
    "karma",
    "heroku"
  ],
  "dependencies": {
    "@slack/client": "^5.0.2",
    "@slack/web-api": "^6.8.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "handlebars": "^4.7.6",
    "knex": "^2.3.0",
    "lodash.camelcase": "^4.3.0",
    "mime": "^2.3.1",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/mime": "^2.0.3",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.11.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "coveralls": "^3.0.2",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-tdmalone": "^0.1.3",
    "eslint-plugin-dollar-sign": "^1.0.1",
    "eslint-plugin-jest": "^21.23.0",
    "jest": "^23.4.2",
    "jest-chain": "^1.0.3",
    "jest-extended": "^0.11.5",
    "nodemon": "^2.0.20",
    "object-assign-deep": "^0.4.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4"
  }
}
